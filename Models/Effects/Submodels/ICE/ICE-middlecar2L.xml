<?xml version="1.0"?>
<PropertyList>

	<path>ICE3middlecar2.ac</path>
	
	<offsets>
		<x-m>0</x-m>
		<y-m>0</y-m>
		<z-m>0</z-m>
		<pitch-deg>0</pitch-deg>
	</offsets>

	<animation>
		<enable-hot type="bool">false</enable-hot>
	</animation>

	<animation>
		<type>range</type>
		<min-m>0</min-m>
		<max-m>20000</max-m>
	</animation>

  <animation>
    <type>material</type>
    <object-name>Cube.001_1</object-name>
    <condition>
      <less-than>
        <property>/sim/time/sun-angle-rad</property>
        <value>1.57</value>
      </less-than>
    </condition>
    <emission>
      <red>   0.0</red>
      <green> 0.0 </green>
      <blue>  0.0 </blue>
    </emission>
  </animation>
  
  
<animation>
    <type>material</type>
    <object-name>Cube.001_1</object-name>
    <condition>
      <greater-than>
        <property>/sim/time/sun-angle-rad</property>
        <value>1.57</value>
      </greater-than>
    </condition>
    <emission>
      <red>   0.35</red>
      <green>0.35 </green>
      <blue> 0.35 </blue>
    </emission>
  </animation>
 
 <!-- leftright -->    
    
    <animation>
    <type>translate</type>
    
    <property>sim/multiplay/generic/float[3]</property>
    <interpolation>
      <entry><ind> 132000 </ind><dep>  2 </dep></entry>
      <entry><ind> 115000 </ind><dep>  2 </dep></entry>
      <entry><ind> 99000 </ind><dep>  3.5 </dep></entry> 
      <entry><ind> 82500 </ind><dep>  3.5 </dep></entry>
      <entry><ind> 66001 </ind><dep>  0.0 </dep></entry>
      <entry><ind> 0 </ind><dep>  0.0 </dep></entry>
    </interpolation>    
    <axis>
      <x>  0 </x>
      <y> 1 </y>
      <z>  0 </z>
    </axis>
  </animation>
  
  <!-- up-down -->
    
  <animation>
    <type>translate</type>
    
    <property>sim/multiplay/generic/float[3]</property>
    <interpolation>
      <entry><ind> 132000 </ind><dep>  0.0 </dep></entry>
      <entry><ind> 115000 </ind><dep>  0.0 </dep></entry>
      <entry><ind> 90000 </ind><dep>  0.0 </dep></entry> 
      <entry><ind> 75500 </ind><dep>  3.40 </dep></entry>
      <entry><ind> 66001 </ind><dep>  3.40 </dep></entry>
      <entry><ind> 0 </ind><dep>  3.4 </dep></entry>
    </interpolation>    
    <axis>
      <x>  0 </x>
      <y> 0 </y>
      <z>  -1 </z>
    </axis>
  </animation>

  <!-- forward-backward -->
  
 <animation>
   <type>translate</type>
   
   <property>sim/multiplay/generic/float[3]</property>
   <interpolation>
      <entry><ind> 132000 </ind><dep>  -11.13 </dep></entry>
      <entry><ind> 115000 </ind><dep>  -11.13 </dep></entry>
      <entry><ind> 99000 </ind><dep> 30.0 </dep></entry> 
      <entry><ind> 82500 </ind><dep> 45.0 </dep></entry>
      <entry><ind> 66001 </ind><dep> 60.0 </dep></entry>
      <entry><ind> 0 </ind><dep>  60.0 </dep></entry>
    </interpolation>    
   <axis>
     <x> -1 </x>
     <y>  0 </y>
     <z>  0 </z>
   </axis>
 </animation>


</PropertyList>
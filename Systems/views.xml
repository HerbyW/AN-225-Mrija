<PropertyList>

  <chase-distance-m type="double" archive="y"> -75 </chase-distance-m>

  <view n="100">
    <name>Co Pilote</name>
    <type>lookfrom</type>
    <internal archive="y">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <ground-level-nearplane-m type="double">0.01f</ground-level-nearplane-m>
      <x-offset-m archive="y">   0.65</x-offset-m> <!-- Right -->
      <y-offset-m archive="y">  4.37 </y-offset-m> <!-- Up    -->
      <z-offset-m archive="y"> -22.16 </z-offset-m> <!-- Back  -->
      <pitch-offset-deg> -5 </pitch-offset-deg>
      <default-field-of-view-deg type="double"> 85 </default-field-of-view-deg>
    </config>
  </view>

  <view n="101">
    <name>Engineer</name>
    <type>lookfrom</type>
    <internal archive="y">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <ground-level-nearplane-m type="double">0.01f</ground-level-nearplane-m>
      <x-offset-m archive="y">   0.790 </x-offset-m> <!-- Right -->
      <y-offset-m archive="y">  4.0 </y-offset-m> <!-- Up    -->
      <z-offset-m archive="y"> -18.74 </z-offset-m> <!-- Back  -->
      <pitch-offset-deg> -2 </pitch-offset-deg>
      <default-field-of-view-deg type="double"> 80 </default-field-of-view-deg>
      <heading-offset-deg type="double">180 </heading-offset-deg>
    </config>
  </view> 
  
  <view n="102">
    <name>Cargo Ramp</name>
    <type>lookfrom</type>
    <internal archive="y">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <ground-level-nearplane-m type="double">0.01f</ground-level-nearplane-m>
      <x-offset-m archive="y">   2.70 </x-offset-m> <!-- Right -->
      <y-offset-m archive="y">  0.80 </y-offset-m> <!-- Up    -->
      <z-offset-m archive="y"> -20.72 </z-offset-m> <!-- Back  -->
      <pitch-offset-deg> -5 </pitch-offset-deg>
      <default-field-of-view-deg type="double"> 105 </default-field-of-view-deg>
      <heading-offset-deg type="double">10 </heading-offset-deg>
    </config>
  </view>  
  
  
  
  <view n="0">
    <internal archive="y">true</internal>
    <config>
      <x-offset-m archive="y">-0.58</x-offset-m>
        <y-offset-m archive="y">4.37</y-offset-m>
        <z-offset-m archive="y">-22.16</z-offset-m>
      <pitch-offset-deg> -5 </pitch-offset-deg>
      <default-field-of-view-deg type="double"> 95 </default-field-of-view-deg>
    </config>
  </view>

  <view n="1">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

  <view n="2">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

  <view n="3">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

  <view n="4">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

  <view n="5">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>
  
  <view n="6">
    <config>
      <target-z-offset-m archive="y" type="double"> 0 </target-z-offset-m>
    </config>
  </view>

</PropertyList>
